<Window x:Class="CC.FecharVaga"
         xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CC.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Background="#eff2f7">
    <Grid>
        <StackPanel>
            <ListView x:Name="listaUsuarios" d:ItemsSource="{d:SampleData ItemCount=5}">
                <ListView.View>
                    <GridView>
                        <GridViewColumn/>
                    </GridView>
                </ListView.View>
            </ListView>
        </StackPanel>
        <StackPanel Orientation="Horizontal" VerticalAlignment="Bottom" Margin="1400 10 0 10" Grid.Row="4">
            <Button Content="Voltar" Grid.Row="6" HorizontalAlignment="Left" Margin="-1350 40 0 0" Width="150" Style="{StaticResource Secondarybutton}" />
            <Button Content="Fechar Vaga" Grid.Row="6" HorizontalAlignment="Left" Margin="-800 40 0 0" Width="150" Style="{StaticResource Mainbutton}" />
        </StackPanel>
    </Grid>
</Window>
